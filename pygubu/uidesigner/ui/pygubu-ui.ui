<?xml version="1.0" ?>
<interface>
  <object class="tk.Menu" id="mainmenu">
    <property name="tearoff">0</property>
    <child>
      <object class="tk.Menuitem.Submenu" id="filemenu">
        <property name="label">File</property>
        <property name="tearoff">0</property>
        <property name="underline">0</property>
        <child>
          <object class="tk.Menuitem.Command" id="filenew">
            <property name="command">on_menuitem_new_clicked</property>
            <property name="label">New</property>
            <property name="command_id_arg">False</property>
          </object>
        </child>
        <child>
          <object class="tk.Menuitem.Command" id="fileopen">
            <property name="command">on_menuitem_open_clicked</property>
            <property name="label">Open …</property>
          </object>
        </child>
        <child>
          <object class="tk.Menuitem.Command" id="filesave">
            <property name="command">on_menuitem_save_clicked</property>
            <property name="label">Save …</property>
          </object>
        </child>
        <child>
          <object class="tk.Menuitem.Command" id="filesaveas">
            <property name="command">on_menuitem_saveas_clicked</property>
            <property name="label">Save as …</property>
            <property name="command_id_arg">False</property>
          </object>
        </child>
        <child>
          <object class="tk.Menuitem.Separator" id="separator1"/>
        </child>
        <child>
          <object class="tk.Menuitem.Command" id="filequit">
            <property name="command">on_menuitem_quit_clicked</property>
            <property name="label">Quit …</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="tk.Menuitem.Submenu" id="editmenu">
        <property name="label">Edit</property>
        <property name="command_id_arg">False</property>
        <property name="tearoff">0</property>
        <property name="underline">0</property>
        <child>
          <object class="tk.Menuitem.Command" id="edit_item_delete">
            <property name="accelerator">Del</property>
            <property name="command">on_edit_menuitem_clicked</property>
            <property name="label">Item delete</property>
            <property name="command_id_arg">True</property>
          </object>
        </child>
        <child>
          <object class="tk.Menuitem.Separator" id="edit_Separator1"/>
        </child>
        <child>
          <object class="tk.Menuitem.Command" id="edit_item_up">
            <property name="accelerator">Ctrl+i</property>
            <property name="command">on_edit_menuitem_clicked</property>
            <property name="label">Item up</property>
            <property name="command_id_arg">True</property>
          </object>
        </child>
        <child>
          <object class="tk.Menuitem.Command" id="edit_item_down">
            <property name="accelerator">Ctrl+k</property>
            <property name="command">on_edit_menuitem_clicked</property>
            <property name="label">Item down</property>
            <property name="command_id_arg">True</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="tk.Menuitem.Submenu" id="helpmenu">
        <property name="label">Help</property>
        <property name="command_id_arg">False</property>
        <property name="tearoff">0</property>
        <property name="underline">0</property>
        <child>
          <object class="tk.Menuitem.Command" id="help_online">
            <property name="command">on_help_menuitem_clicked</property>
            <property name="label">Help (Online)</property>
            <property name="command_id_arg">True</property>
          </object>
        </child>
        <child>
          <object class="tk.Menuitem.Command" id="help_about">
            <property name="command">on_help_menuitem_clicked</property>
            <property name="label">About …</property>
            <property name="command_id_arg">True</property>
          </object>
        </child>
      </object>
    </child>
  </object>
  <object class="ttk.Frame" id="mainwindow">
    <property name="padding">2</property>
    <layout>
      <property name="column">0</property>
      <property name="row">0</property>
      <property name="sticky">nsew</property>
